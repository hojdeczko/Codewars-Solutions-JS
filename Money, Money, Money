// Link to the kata: https://www.codewars.com/kata/563f037412e5ada593000114

function calculateYears(principal, interest, tax, desired) {
    
    var endOfYear = principal;
    var years = 0;
    
        while (endOfYear < desired) {
          endOfYear = endOfYear * interest - endOfYear * interest * tax + endOfYear;
          years++;
          };
        
      
    return years;  
} 
